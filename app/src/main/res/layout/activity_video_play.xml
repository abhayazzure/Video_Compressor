<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_dark"
    android:keepScreenOn="true">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:id="@+id/card_thumb"
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    app:cardBackgroundColor="@color/bg_dark"
                    app:cardCornerRadius="0dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:id="@+id/videoview_container"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:visibility="visible">

                            <VideoView
                                android:id="@+id/video_view"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentBottom="true" />

                            <View
                                android:id="@+id/list_item_video_clicker_small"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:background="@drawable/bg_card_transparent"
                                android:clickable="true"
                                android:focusable="true"
                                android:visibility="gone" />


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_play"
                                    android:layout_width="0dp"
                                    android:layout_height="@dimen/_25sdp"
                                    android:layout_weight="0.1"
                                    android:src="@drawable/ic_play_vector"
                                    app:tint="@color/white" />

                                <SeekBar
                                    android:id="@+id/play_progress_bar"
                                    android:layout_width="0dp"
                                    android:layout_height="@dimen/_25sdp"
                                    android:layout_weight="1"
                                    android:maxHeight="1dp"
                                    android:progress="0"
                                    android:progressDrawable="@drawable/seekbar_progress_blue"
                                    android:thumb="@drawable/custom_thumb_blue" />

                                <TextView
                                    android:id="@+id/tv_video_duration"
                                    android:layout_width="wrap_content"
                                    android:layout_height="@dimen/_25sdp"
                                    android:layout_marginEnd="@dimen/_8sdp"
                                    android:text="@string/_00_15_00_30"
                                    android:textColor="@color/white"
                                    android:textStyle="bold" />

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/ry_thumb_container"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:focusable="true"
                            android:visibility="gone">

                            <ImageView
                                android:id="@+id/iv_video_thumb"
                                android:layout_width="match_parent"
                                android:layout_height="300dp"
                                android:scaleType="centerCrop"
                                android:visibility="visible" />

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:background="@drawable/seekbar_gradient_bg">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginEnd="10dp"
                                    android:fontFamily="@font/montserratmedium"
                                    android:textColor="@color/white" />

                                <SeekBar
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:maxHeight="2dp"
                                    android:minHeight="2dp"
                                    android:progressDrawable="@drawable/seekbar_progress"
                                    android:thumb="@drawable/custom_thumb"
                                    android:visibility="invisible" />

                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:padding="15dp">

                                    <ImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp" />
                                </RelativeLayout>
                            </RelativeLayout>
                        </RelativeLayout>

                        <ImageView
                            android:id="@+id/iv_play"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@drawable/ic_play"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/btn_back"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:layout_alignParentStart="true"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:paddingLeft="12dp"
                            android:paddingTop="12dp"
                            android:paddingRight="12dp"
                            android:paddingBottom="12dp"
                            android:src="@drawable/ic_back_new"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:tint="@color/white" />
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>

                <!--                <androidx.constraintlayout.widget.ConstraintLayout-->
                <!--                    android:layout_width="match_parent"-->
                <!--                    android:layout_height="match_parent"-->
                <!--                    android:layout_marginTop="@dimen/_20sdp">-->

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/success_components_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/card_thumb"
                    android:paddingBottom="@dimen/_5sdp"
                    android:visibility="visible"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="parent">

                    <LinearLayout
                        android:id="@+id/btn_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ly_custom">

                        <HorizontalScrollView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:scrollbars="none">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <LinearLayout
                                    android:id="@+id/imgShare"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/_10sdp">

                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center_horizontal"
                                        android:contentDescription="@string/app_name"
                                        android:gravity="center_horizontal"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/ic_share_other" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:fontFamily="@font/montserratmedium"
                                        android:gravity="center"
                                        android:text="string/other"
                                        android:textColor="@color/white"
                                        android:textSize="11sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/imgmailShare"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/_10sdp">

                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center_horizontal"
                                        android:contentDescription="@string/app_name"
                                        android:gravity="center_horizontal"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/ic_share_mail" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:fontFamily="@font/montserratmedium"
                                        android:gravity="center"
                                        android:text="string/mail"
                                        android:textColor="@color/white"
                                        android:textSize="11sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/imgWhatsApp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/_10sdp">

                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center_horizontal"
                                        android:contentDescription="@string/app_name"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/ic_whatsapp" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:fontFamily="@font/montserratmedium"
                                        android:gravity="center"
                                        android:text="string/whasapp"
                                        android:textColor="@color/white"
                                        android:textSize="11sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/imgFacebook"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/_10sdp">

                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center_horizontal"
                                        android:contentDescription="@string/app_name"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/ic_facebook" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:fontFamily="@font/montserratmedium"
                                        android:gravity="center"
                                        android:text="string/facebook"
                                        android:textColor="@color/white"
                                        android:textSize="11sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/imgMessenger"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/_10sdp">

                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center_horizontal"
                                        android:contentDescription="@string/app_name"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/ic_messenger" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:fontFamily="@font/montserratmedium"
                                        android:gravity="center"
                                        android:text="string/messenger"
                                        android:textColor="@color/white"
                                        android:textSize="11sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/imgInstagram"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/_10sdp">

                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center_horizontal"
                                        android:contentDescription="@string/app_name"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/ic_instagram" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:fontFamily="@font/montserratmedium"
                                        android:gravity="center"
                                        android:text="string/instagram"
                                        android:textColor="@color/white"
                                        android:textSize="11sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/imgTwitter"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/_10sdp">

                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center_horizontal"
                                        android:contentDescription="@string/app_name"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/ic_twitter" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:fontFamily="@font/montserratmedium"
                                        android:gravity="center"
                                        android:text="string/twitter"
                                        android:textColor="@color/white"
                                        android:textSize="11sp" />
                                </LinearLayout>
                            </LinearLayout>
                        </HorizontalScrollView>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <ImageButton
                                android:id="@+id/btn_play"
                                android:layout_width="36dp"
                                android:layout_height="36dp"
                                android:background="@color/white"
                                android:src="@drawable/ic_replace"
                                app:layout_constraintEnd_toStartOf="@+id/btn_share"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tv_success_message"
                                app:tint="@color/bg_color" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="9dp"
                                android:fontFamily="@font/montserratmedium"
                                android:gravity="center"
                                android:letterSpacing="0.06"
                                android:text="string/replace"
                                android:textAllCaps="true"
                                android:textColor="#777777"
                                android:textSize="10sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="35dp"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <ImageButton
                                android:id="@+id/btn_done"
                                android:layout_width="36dp"
                                android:layout_height="36dp"
                                android:background="@color/white"
                                android:src="@drawable/ic_save"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toEndOf="@+id/btn_share"
                                app:layout_constraintTop_toTopOf="@+id/btn_share"
                                app:tint="@color/bg_color" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="9dp"
                                android:fontFamily="@font/montserratmedium"
                                android:gravity="center"
                                android:letterSpacing="0.06"
                                android:text="string/save"
                                android:textAllCaps="true"
                                android:textColor="#777777"
                                android:textSize="10sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ly_custom"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/ic_correct" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:fontFamily="@font/montserratmedium"
                                android:gravity="center"
                                android:letterSpacing="0.07"
                                android:paddingLeft="@dimen/_10sdp"
                                android:text="string/saved"
                                android:textColor="@color/white"
                                android:textSize="18sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/tv_custompath"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/montserratmedium"
                            android:gravity="center"
                            android:paddingLeft="@dimen/_8sdp"
                            android:paddingRight="@dimen/_8sdp"
                            android:text=""
                            android:textColor="#777777"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/result_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="63dp"
                        android:layout_marginTop="30dp"
                        android:layout_marginEnd="63dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/btn_container">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/container_original_file_info"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/left_round_gray_bg"
                            app:layout_constraintEnd_toStartOf="@+id/guide1"
                            app:layout_constraintHorizontal_bias="1"
                            app:layout_constraintHorizontal_weight="1"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/tv_original"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:fontFamily="@font/montserratmedium"
                                android:letterSpacing="0.07"
                                android:text="string/original"
                                android:textColor="#828282"
                                android:textSize="10sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/tv_size_original"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/montserratmedium"
                                android:text="24.04MB"
                                android:textColor="#ffffff"
                                android:textSize="16sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tv_original" />

                            <TextView
                                android:id="@+id/tv_original_resolution"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:fontFamily="@font/montserratmedium"
                                android:paddingBottom="10dp"
                                android:text="1280x720"
                                android:textColor="#828282"
                                android:textSize="10sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tv_size_original" />
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guide1"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.5" />

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/container_compressed_file_info"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/right_round_grad_bg"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_weight="1"
                            app:layout_constraintStart_toEndOf="@+id/guide1"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/tv_compressed"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:fontFamily="@font/montserratmedium"
                                android:letterSpacing="0.07"
                                android:text="@string/compressed"
                                android:textColor="@color/white"
                                android:textSize="10sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/tv_size_compressed"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:fontFamily="@font/montserratmedium"
                                android:text="24.04MB"
                                android:textColor="@color/white"
                                android:textSize="16sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tv_compressed" />

                            <TextView
                                android:id="@+id/tv_compressed_resolution"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:fontFamily="@font/montserratmedium"
                                android:paddingBottom="10dp"
                                android:text="1280x720"
                                android:textColor="@color/white"
                                android:textSize="10sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/tv_size_compressed" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginEnd="15dp"
                    android:layout_marginBottom="5dp"
                    android:background="@color/bg_light"
                    app:cardBackgroundColor="#262626"
                    app:cardCornerRadius="@dimen/_5sdp"
                    app:layout_constraintTop_toBottomOf="@+id/success_components_container">

                    <RelativeLayout
                        android:id="@+id/ry_ad_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true">

                        <LinearLayout
                            android:id="@+id/native_container_share"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical" />
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <!--        </androidx.constraintlayout.widget.ConstraintLayout>-->
            </RelativeLayout>
        </androidx.core.widget.NestedScrollView>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/ly_full_video_play"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black"
        android:visibility="gone">

        <ImageView
            android:id="@+id/ivPlayPause"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/ic_video_play_" />

        <View
            android:id="@+id/list_item_video_clicker"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/bg_card_transparent"
            android:clickable="true"
            android:focusable="true"
            android:visibility="visible" />

        <RelativeLayout
            android:id="@+id/ry_seek"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@drawable/seekbar_gradient_bg"
            android:gravity="center_vertical"
            android:padding="@dimen/_10sdp">

            <SeekBar
                android:id="@+id/sbVideo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@+id/tvDuration"
                android:layout_toEndOf="@+id/tvDuration1"
                android:maxHeight="2dp"
                android:minHeight="2dp"
                android:progressDrawable="@drawable/seekbar_progress" />

            <TextView
                android:id="@+id/tvDuration1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/montserratmedium"
                android:gravity="center"
                android:text="01:00"
                android:textColor="@color/white"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvDuration"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginEnd="8dp"
                android:layout_toStartOf="@+id/iv_exitfullscreen"
                android:fontFamily="@font/montserratmedium"
                android:gravity="center_vertical"
                android:text="01:00"
                android:textColor="@color/white"
                android:textSize="14sp" />

            <RelativeLayout
                android:id="@+id/iv_exitfullscreen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:padding="15dp">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_fullscreen"
                    app:tint="@color/white" />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
